<!--Change refentry id and indexterm id-->
<refentry id="cntCycles">
<indexterm id="IndexCntCycles"><primary>cntCycles</primary></indexterm>
  <refentryinfo><title>Instrument Control:Sensing and Control</title></refentryinfo>
  <refmeta>
    <refentrytitle>cntCycles</refentrytitle>
  </refmeta>

  <refnamediv>
    <refname>cntCycles</refname>
    <refpurpose>
      Retourne le nombre de fois qu'un compteur a bouclé.
    </refpurpose>
  </refnamediv>
 
  <refsect1>
    <title>Description</title>
    <para>Opcode du greffon counter.</para>
     <para>
      Retourne le nombre de fois qu'un compteur a bouclé.
    </para>
  </refsect1>

  <refsect1>
    <title>Syntaxe</title>
    <synopsis>kval <command>cntCycles</command> icnt</synopsis>
  </refsect1>

  <refsect1>
    <title>Initialisation</title>
    <para>
      <emphasis>icnt</emphasis> -- l'identifiant d'un objet compteur obtenu d'un
      appel à <emphasis>cntCreate</emphasis>.
    </para>
  </refsect1>
 
  <refsect1>
    <title>Exécution</title>
    <para>
      Compte le nombre de fois qu'un compteur a été redémarré.
    </para>
    <para>
      <emphasis>kval</emphasis> -- valeur retournée.
    </para>
  </refsect1>

