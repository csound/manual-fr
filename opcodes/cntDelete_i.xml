<!--Change refentry id and indexterm id-->
<refentry id="cntDelete_i">
<indexterm id="IndexCntDelete_i"><primary>cntDelete_i</primary></indexterm>
<!--Make sure the category below is a valid one! See categories.py-->
  <refentryinfo><title>Instrument Control:Sensing and Control</title></refentryinfo>
  <refmeta>
    <refentrytitle>cntDelete_i</refentrytitle>
  </refmeta>

  <refnamediv>
    <refname>cntDelete_i</refname>
    <refpurpose>
      Efface un compteur.
    </refpurpose>
  </refnamediv>
 
  <refsect1>
    <title>Description</title>
    <para>Opcode du greffon counter.</para>
    <para>
      Efface un compteur et libère la mémoire utilisée.
    </para>
  </refsect1>

  <refsect1>
    <title>Syntaxe</title>
    <synopsis>ival <command>cntDelete_i</command> icnt</synopsis>
  </refsect1>

  <refsect1>
    <title>Initialisation</title>
    <para>
      <emphasis>icnt</emphasis> -- un identifiant pour le compteur obtenu d'un
      appel à <emphasis>cntCreate</emphasis>.
    </para>
  </refsect1>
 
  <refsect1>
    <title>Exécution</title>
    <para>
      <emphasis>ival</emphasis> -- l'identifiant effacé ou un nombre négatif
      si ce compteur n'existait pas.
    </para>
  </refsect1>

  <refsect1>
    <title>Voir aussi</title>
    <para>
      <link linkend="ControlPgmctl"><citetitle>Contrôle séquentiel d'un programme : compteurs</citetitle></link>
    </para>
  </refsect1>

  <refsect1>
